            "cwd": "${fileDirname}",
            "justMyCode": true
        },
        {
            "name": "Tracking GUI",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/tracking_gui.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src/",
            "justMyCode": true
        },
        {
            "name": "Tracking GUI PyQt5",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/Tracking_UI/Ui_Tracker UI.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src/",
            "justMyCode": true
        },
        {
            "name": "UEWS Backend",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/logic_receiving_data.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src/",
            "justMyCode": true
        },
        {
            "name": "Test Server",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/sending_data.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src/",
            "justMyCode": true
        }
    ],
    "compounds":[
        {
            "name": "GUI Test",
            "configurations": ["Tracking GUI PyQt5", "UEWS Backend", "Test Server"],
            "stopAll": true
